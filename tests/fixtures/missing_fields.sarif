{
  "version": "2.1.0",
  "$schema": "https://raw.githubusercontent.com/oasis-tcs/sarif-spec/master/Schemata/sarif-schema-2.1.0.json",
  "runs": [
    {
      "tool": {
        "driver": {
          "name": "CodeQL"
        }
      },
      "results": [
        {
          "ruleId": "unknown/rule",
          "message": { "text": "Something is wrong." }
        },
        {
          "ruleId": "another/rule",
          "level": "error",
          "message": { "text": "Another issue." },
          "locations": [
            {
              "physicalLocation": {
                "artifactLocation": { "uri": "src/app.js" }
              }
            }
          ]
        }
      ]
    }
  ]
}
